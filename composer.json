{
    "name": "keystone/tactician-deferred",
    "description": "A Tactician middleware to allow commands to be handled asynchronously",
    "keywords": ["tactician", "asynchronous", "queue", "deferrable"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "league/tactician": "^1.0",
        "keystone/queue": "^0.1@alpha"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "^0.9"
    },
    "autoload": {
        "psr-4": {
            "Keystone\\Tactician\\Deferred\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keystone\\Tactician\\Deferred\\": "tests/"
        }
    }
}
